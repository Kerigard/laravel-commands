{
    "name": "kerigard/laravel-commands",
    "description": "Commands for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Vladislav Sidelnikov",
            "email": "kerihoros@gmail.com"
        }
    ],
    "homepage": "https://github.com/kerigard/laravel-commands",
    "keywords": [
        "laravel"
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "orchestra/testbench": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Kerigard\\LaravelCommands\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kerigard\\LaravelCommands\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kerigard\\LaravelCommands\\CommandsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}